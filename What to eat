#include<stdio.h>
int main()
{
  int choice;
  printf("enter any number from 1 to 5");
  scanf("%d",&choice);
  switch(choice)
  {
    case 1:printf("Food item:Pasta \n Price:179"); break;
    case 2:printf("Food item:Pizza \n Price:239"); break;
    case 3:printf("Food item:Sandwich \n Price:149"); break;
    case 4:printf("Food item:Burger \n Price:129"); break;
    case 5:printf("Food item:French Fries \n Price:99"); break;
  }
  return 0;
}
